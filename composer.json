{
	"name": "vasek-purchart/phing-tester",
	"description": "Helper tool for writing Phing component tests using PHPUnit",
	"keywords": ["phing", "test", "phpunit"],
	"license": "MIT",
	"authors": [
		{
			"name": "Va≈°ek Purchart",
			"email": "me@vasekpurchart.cz",
			"homepage": "http://vasekpurchart.cz"
		}
	],
	"require": {
		"php": "~7.0",
		"phing/phing": "~2.16",
		"phpunit/phpunit": "~6.0"
	},
	"require-dev": {
		"consistence/coding-standard": "2.2.1",
		"jakub-onderka/php-console-highlighter": "0.3.2",
		"jakub-onderka/php-parallel-lint": "0.9.2"
	},
	"autoload": {
		"psr-4": {
			"VasekPurchart\\Phing\\PhingTester\\": [
				"src"
			]
		},
		"classmap": [
			"src"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"VasekPurchart\\Phing\\PhingTester\\": [
				"tests"
			]
		},
		"classmap": [
			"tests"
		]
	},
	"config": {
		"bin-dir": "bin"
	}
}
